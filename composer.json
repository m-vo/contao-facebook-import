{
  "name": "mvo/contao-facebook-import",
  "type": "contao-bundle",
  "description": "Facebook posts and events for Contao Open Source CMS",
  "keywords": [
    "contao",
    "facebook",
    "import",
    "social",
    "posts",
    "events"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Vondano",
      "homepage": "https://github.com/m-vo/contao-facebook-import"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-PDO": "*",
    "ext-json": "*",
    "contao/core-bundle": "^4.4",
    "facebook/graph-sdk": "^5.7",
    "guzzlehttp/guzzle": "~6.0",
    "codefog/contao-haste": "^4.17",
    "symfony/stopwatch": "^3.0|^4.0",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpstan/phpstan": "^0.11.8",
    "phpstan/extension-installer": "^1.0.1",
    "phpstan/phpstan-symfony": "^0.11.6",
    "phpstan/phpstan-doctrine": "^0.11.5"
  },
  "autoload": {
    "psr-4": {
      "Mvo\\ContaoFacebookImport\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Mvo\\ContaoFacebookImport\\ContaoManager\\Plugin"
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix"
  }
}